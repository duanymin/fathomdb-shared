option java_package="com.fathomdb.proxy.protobuf";

message VfsItemModel {
	optional string name = 1;
	optional bytes hash = 2;
	optional uint64 length = 3;
	optional uint64 last_modified = 4;
	
	enum WellKnownContentType {
	    DIRECTORY = 0;
  	}
  	optional WellKnownContentType well_known_content_type = 5;
   
	optional string content_type = 6;
	
	repeated VfsItemModel child = 7;
}